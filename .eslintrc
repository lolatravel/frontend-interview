{
    "extends": ["eslint:recommended"],
    "plugins": ["react"],
    "parser": "babel-eslint",
    "rules": {
        "no-console": "off",
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error"
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
  }
  